{
  "name": "site",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=23.0.0"
  },
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "deploy": "wrangler deploy"
  },
  "devDependencies": {
    "@prgm/sveltekit-progress-bar": "^3.0.2",
    "@sveltejs/adapter-cloudflare": "^7.0.5",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^24.0.10",
    "@types/three": "^0.178.0",
    "cssnano": "^7.0.7",
    "mdsvex": "^0.12.3",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "reading-time": "^1.5.0",
    "shiki": "^3.7.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^6.2.6",
    "wrangler": "^4.24.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "esm-env": "^1.2.2",
    "sharp": "^0.34.3",
    "temporal-polyfill": "^0.3.0",
    "three": "^0.178.0"
  }
}
